package com.local.lessons.basics;

public class InterationControl {

	/*
	 
	*/
	public static void main(String[] args) {
		int count = 0;
		int end = 10;
		//for loop
		for(count=0;count<end;count++) {
			System.out.println(count);
		}

		// while loop
		end = 20;
		while(count<end){
			System.out.println(count);
			count++;
		}
		

	}

}
