/**
 * 
 */
package com.local.lessons.basics;

/**
 * @author Zero
 *
 */
public class PrimitiveBasics {

	/**
	 * Basics of Primitives 
	 */
	public static void main(String[] args) {
		
		System.out.println("Integers Types: ");
		byte var1 = 21;
		System.out.println("Byte: " + var1);
		short var2 = 1;
		System.out.println("Short: " + var2);
		int var3 = 123;
		System.out.println("Int: " + var3);
		long var4 = 231;
		System.out.println("Long: " + var4);
		
		System.out.println("Characters Types ");
		String var5 = "String isn\'t a primitive it is an object";
		System.out.println("This is a String: "+ var5);
		char var6 = 'C';
		System.out.println("This is a char" + var6);
		
		
		

	}

}
